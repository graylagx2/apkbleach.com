{
    "name": "ApkBleach",
    "short_name": "apkbleach",
    "description": "Obfuscates android payloads generated by the metasploit-framework",
    "version": "0.1",
    "manifest_version": 2,
    "default_locale": "en-GB",
    "author": "graylagx2",
    "start_url": "/",
    "scope": "/",
    "display": "standalone",
    "orientation": "portrait",
    "theme_color": "#444444",
    "background_color": "#444444",
    "icons": [
        {
            "src": "/static/images/icons/bleach_logo_ldpi.png",
            "sizes": "512x512",
            "type": "image/png"
        }
    ],
    "app": {
        "permissions": [
            "unlimitedStorage",
            "notifications",
            "fullscreen"
        ]
    }
}