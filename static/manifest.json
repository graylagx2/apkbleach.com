{
    "name": "ApkBleach",
    "short_name": "ApkBleach",
    "description": "Obfuscates android payloads generated by the metasploit-framework",
    "version": "0.1",
    "manifest_version": 2,
    "default_locale": "en-GB",
    "author": "graylagx2",
    "start_url": "/",
    "scope": "/",
    "display": "standalone",
    "orientation": "portrait",
    "theme_color": "#444444",
    "background_color": "#444444",
    "icons": [
        {
            "src": "/static/images/icons/bleach_logo512.png",
            "sizes": "512x512",
            "type": "image/png",
            "purpose": "any"
        }
        {
            "src": "/static/images/icons/maskable_bleach_logo512.png",
            "sizes": "1965x1965",
            "type": "image/png",
            "purpose": "maskable"
        }
    ],
    "app": {
        "permissions": [
            "unlimitedStorage",
            "notifications",
            "fullscreen"
        ]
    }
}